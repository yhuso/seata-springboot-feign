<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aha.tech.model.mapper.MessageLogMapper">
    <resultMap id="BaseResultMap" type="com.aha.tech.model.entity.MessageLogEntity">
        <!--
          WARNING - @mbg.generated
        -->
        <id column="id" jdbcType="BIGINT" property="id"/>
        <result column="uuid" jdbcType="VARCHAR" property="uuid"/>
        <result column="topic" jdbcType="VARCHAR" property="topic"/>
        <result column="body" jdbcType="VARCHAR" property="body"/>
        <result property="status" column="status"/>
        <result property="processBean" column="process_bean"/>
        <result property="processMethod" column="process_method"/>
        <result property="retryNum" column="retry_num"/>

    </resultMap>
    <select id="selectCompensation" resultMap="BaseResultMap">
        select id,
               uuid,
               status,
               topic,
               body,
               process_method,
               process_bean,
               retry_num
        from h_message_log
        where id > #{latestId}
          and status = 0
          and retry_num > 0
        order by id
        limit 0,50

    </select>
</mapper>